<?xml version="1.0"?>
<extension key="io.compuco.automateddirectdebit" type="module">
  <file>automateddirectdebit</file>
  <name>AutomatedDirectDebit</name>
  <description>Automated Direct Debit</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compuco.io</email>
  </maintainer>
  <urls>
    <url desc="Documentation">https://github.com/compucorp/io.compuco.automateddirectdebit/blob/master/README.md</url>
  </urls>
  <releaseDate>2025-02-20</releaseDate>
  <version>1.2.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <comments>
    Supported CiviCRM versions  :
    - A patched version of CiviCRM 5.51.3, which can be found here: https://github.com/compucorp/civicrm-core/releases
  </comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Automateddirectdebit</namespace>
    <format>23.02.1</format>
    <angularModule>crmAutomateddirectdebit</angularModule>
  </civix>
  <requires>
    <ext>uk.co.compucorp.membershipextras</ext>
  </requires>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Automateddirectdebit_Upgrader</upgrader>
</extension>
